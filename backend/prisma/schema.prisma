generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int @id @default(autoincrement())
  name  String
  email String @unique
  password String 
}

model Products {
  id            Int     @id @default(autoincrement())
  name          String
  price         Float
  rating        Float?
  stockQuantity Int
}


